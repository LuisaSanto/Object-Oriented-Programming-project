#
# Script to run java text editor
# -Dimport="dailies/$TEST.import" 

clear

TEST=dailies/A-012-001-M-ok
        echo "$TEST";
        if [ -e "$TEST.import" ]
            then
                java -Dimport="$TEST.import" -Din="$TEST.in" -Dout="$TEST.outhyp" -cp "po-uilib/po-uilib.jar:edt-support/edt-support.jar:edt-core/edt-core.jar:edt-textui/edt-textui.jar" edt.textui.TextEditor
            else
                java -Din="$TEST.in" -Dout="$TEST.outhyp" -cp "po-uilib/po-uilib.jar:edt-support/edt-support.jar:edt-core/edt-core.jar:edt-textui/edt-textui.jar" edt.textui.TextEditor
        fi
    
diff $TEST.out $TEST.outhyp